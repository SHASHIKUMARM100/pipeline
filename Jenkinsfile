pipeline {
    agent any

    stages {
        stage('Git Checkout Stage') {
            steps {
                echo 'This is Git Checkout Stage'
            }
        }
		stage('Build Stage') {
            steps {
                echo 'This is Build Stage'
            }
        }
		stage('Push to Artifact stage') {
            steps {
                echo 'This is Push to Artifact stage'
            }
        }
		stage('Deploy stage') {
            steps {
                echo 'This is Deploy Stage'
            }
        }
    }
}
